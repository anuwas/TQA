<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="triangle" class="com.anu.tqa.autowiring.Triangle" autowire="byName">
	
</bean>

<bean id="pointA" class="com.anu.tqa.autowiring.Point">
	<property name="pointX" value="0"/>
	<property name="pointY" value="0"/>
</bean>

<bean id="pointB" class="com.anu.tqa.autowiring.Point">
	<property name="pointX" value="0"/>
	<property name="pointY" value="20"/>
</bean>

<bean id="pointC" class="com.anu.tqa.autowiring.Point">
	<property name="pointX" value="20"/>
	<property name="pointY" value="0"/>
</bean>
</beans>
